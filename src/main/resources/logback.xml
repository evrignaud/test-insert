<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appender name="CONSOLE-stdout" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%-5level - %msg%n</pattern>
        </encoder>
    </appender>
    <root level="WARN">
        <appender-ref ref="CONSOLE-stdout" />
    </root>

    <logger name="org.mongodb" level="WARN" />

</configuration>
